<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SesameCreditView">
        <!--<attr name="text" format="string" />
        <attr name="textColor" format="color" />-->
        <attr name="outRectRadius" format="float" />
        <attr name="MiddleRectRadius" format="float" />
        <attr name="innerRectRadius" format="float" />
        <attr name="ratio" format="integer" />
    </declare-styleable>


    <declare-styleable name="ProgressView">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="radius" format="dimension" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="innerColor" format="color" />
        <attr name="outerColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="maxProgress" format="integer" />
        <attr name="currentProgress" format="integer" />
    </declare-styleable>

    <declare-styleable name="RoundImageViewByXfermode">
        <attr name="borderRadius" format="dimension"/>
        <attr name="type" />
    </declare-styleable>

    <attr name="type">
        <enum name="circle" value="0" />
        <enum name="round" value="1"/>
    </attr>
</resources>